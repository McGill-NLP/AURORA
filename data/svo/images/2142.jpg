            function checkUser(e = null) {
                const logged = window.Pelcro.user.isAuthenticated();
                const logins = document.querySelectorAll( '.plus-login-button' );

                if( logged ) {
                    document.body.classList.add('Pelcro--isLogged');
                }else{
                    document.body.classList.remove('Pelcro--isLogged');
                }

                Array.from(logins).map(login => {
                    login.innerText = logged ? 'My account' : 'Log in';
                    login.setAttribute('alt', logged ? 'My account' : 'Log in');
                    login.setAttribute('title', logged ? 'My account' : 'Log in');
                });
            }

            ['PelcroUserLoggedIn', 'PelcroUserLogout']
                .map( ev => document.addEventListener( ev, checkUser ) );

            checkUser();
